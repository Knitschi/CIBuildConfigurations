include(cpf-CI)
[settings]
os=Linux
os_build=Linux
arch=x86_64
arch_build=x86_64
compiler=clang
compiler.version=10
build_type=Debug
compiler.libcxx=libstdc++11
[options]
CPF_CONFIG=clang-x86_64-shared-debug
shared=True
CMAKE_C_COMPILER=/usr/bin/clang-10
CMAKE_CXX_COMPILER=/usr/bin/clang++-10
CMAKE_GENERATOR=Unix Makefiles
CPF_ENABLE_CLANG_FORMAT_TARGETS=TRUE
CPF_ENABLE_CLANG_TIDY_TARGET=TRUE
[build_requires]
[env]